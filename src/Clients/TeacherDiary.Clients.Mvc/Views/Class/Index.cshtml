@model IEnumerable<ClassViewModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    @foreach(var clas in Model)
    { 
        <div>
            @(Html.ActionLink<ClassController>(clas.Name, c => c.Students(clas.Id), new { area = string.Empty }, new { @class = "btn btn-dark" }))
        </div>
    }
</div>